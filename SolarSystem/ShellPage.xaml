<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:SolarSystem"
             x:Class="SolarSystem.ShellPage">
    
    <Shell.FlyoutHeader>
        <Frame>
            <View/>
        </Frame>
    </Shell.FlyoutHeader>
    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <Tab Title="Tata" Icon="icon_t.png">
            <ShellContent Title="Planets">
                    <views:PlanetsPage/>
            </ShellContent>
            <ShellContent Title="Stars">
                <views:StarPage/>
            </ShellContent>
            <ShellContent Title="Asteroids">
                <views:AsteroidPage/>
            </ShellContent>
        </Tab>
        
    </FlyoutItem>
    

</Shell>